{% extends '../../../../../views/layout.twig' %}

{% import '../../blocks/user/index.twig' as user %}

{% block title %}Users{% endblock %}

{% block content %}
    <h1>Users</h1>

    <p>
        <a href="{{ path('users_json') }}">See JSON</a>
    </p>

    {% for userParams in data.users %}
        {{ user.render(userParams, {
            withLink: true,
        }) }}
    {% else %}
        <p>No users found.</p>
    {% endif %}
{% endblock %}
