{
    "route_without_controller": {
        "name": "route_without_controller",
        "path": "/route-without-controller/",
        "defaults": {},
        "requirements": {},
        "methods": [
            "all"
        ]
    },

    "route_simple": {
        "name": "route_simple",
        "path": "/route-simple/",
        "defaults": {
            "_controller": "controllers/default.js"
        },
        "requirements": {},
        "methods": [
            "all"
        ]
    },

    "route_for_html_without_template": {
        "name": "route_for_html_without_template",
        "path": "/route-for-html-without-template/",
        "defaults": {
            "_controller": "controllers/default.js",
            "_format": "html"
        },
        "requirements": {},
        "methods": [
            "all"
        ]
    },

    "route_with_template_without_format": {
        "name": "route_with_template_without_format",
        "path": "/route-with-template-without-format/",
        "defaults": {
            "_controller": "controllers/default.js",
            "_template": "views/hello_world.html.twig"
        },
        "requirements": {},
        "methods": [
            "all"
        ]
    },

    "route_with_template_for_json": {
        "name": "route_with_template_for_json",
        "path": "/route-with-template-for-json/",
        "defaults": {
            "_controller": "controllers/default.js",
            "_template": "views/hello_world.html.twig",
            "_format": "json"
        },
        "requirements": {},
        "methods": [
            "all"
        ]
    },

    "route_with_param": {
        "name": "route_with_param",
        "path": "/route-with-param/{param}/",
        "defaults": {
            "_controller": "controllers/queryResolver.js",
            "_template": "views/queryResolver.html.twig",
            "param": "val1"
        },
        "requirements": {
            "param": "val1|val2"
        },
        "methods": [
            "all"
        ]
    },

    "only_post_method_allowing": {
        "name": "only_post_method_allowing",
        "path": "/only-post-method-allowing/",
        "defaults": {
            "_controller": "controllers/default.js"
        },
        "requirements": {},
        "methods": [
            "POST"
        ]
    },

    "external_route_simple": {
        "name": "external_route_simple",
        "path": "/external/external-route-simple/",
        "defaults": {
            "_controller": "controllers/queryResolver.js"
        },
        "requirements": {},
        "methods": [
            "all"
        ]
    },

    "external_route_with_param": {
        "name": "external_route_with_param",
        "path": "/external/external-route-with-param/{param}/",
        "defaults": {
            "_controller": "controllers/queryResolver.js",
            "_template": "views/queryResolver.html.twig",
            "param": "val1"
        },
        "requirements": {
            "param": "val1|val2"
        },
        "methods": [
            "all"
        ]
    },

    "external_localized_route_simple": {
        "name": "external_localized_route_simple",
        "path": "/{_locale}/external/external-route-simple/",
        "defaults": {
            "_controller": "controllers/default.js"
        },
        "requirements": {},
        "methods": [
            "all"
        ]
    },

    "external_localized_route_with_param": {
        "name": "external_localized_route_with_param",
        "path": "/{_locale}/external/external-route-with-param/{param}/",
        "defaults": {
            "_controller": "controllers/default.js",
            "param": "val1"
        },
        "requirements": {
            "param": "val1|val2"
        },
        "methods": [
            "all"
        ]
    }
}
