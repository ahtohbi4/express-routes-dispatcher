{
    "external_route_simple": {
        "path": "/external-route-simple/",
        "defaults": {
            "_controller": "controllers/queryResolver.js",
            "_template": "views/queryResolver.html.twig"
        }
    },

    "external_route_with_param": {
        "path": "/external-route-with-param/{param}/",
        "defaults": {
            "_controller": "controllers/queryResolver.js",
            "_template": "views/queryResolver.html.twig",
            "param": "val1"
        },
        "requirements": {
            "param": "val1|val2"
        }
    }
}
